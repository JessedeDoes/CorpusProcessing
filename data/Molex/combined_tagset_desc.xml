                <featureValue desc="Third person">
                    <value>3</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Preferred article (molex feature only)">
            <name>art</name>

                    </posForValue>
                </featureValue>
            </values>
        </feature>
	<feature desc="Case"> <!-- pos nog uitwerken -->
            <name>case</name>
            <values>
               <featureValue desc="Dative">
                    <value>dat</value>
		    <posForValue>
	                <pos>AA</pos>
                        <pos>PD</pos>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos> <!-- etc -->
			<pos>VRB</pos> <!-- ger -->
                    </posForValue>
                </featureValue>
                <featureValue desc="Dative or accusative">
                    <value>dat/acc</value>
                    <posForValue>
			<pos>AA</pos>
                        <pos>PD</pos>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos> <!-- etc -->
                    </posForValue>
                </featureValue>
                <featureValue desc="Genitive">
                    <value>gen</value>
                    <posForValue>
			  <pos>AA</pos>
			  <pos>PD</pos>
                          <pos>NOU-C</pos>
                          <pos>NOU-P</pos> <!-- etc -->
			  <pos>VRB</pos> <!-- ger -->  
                    </posForValue>
	        </featureValue>
               <featureValue desc="Nominative">
                    <value>nom</value>
                    <posForValue>
			  <pos>AA</pos>
                          <pos>NOU-C</pos>
			  <pos>NOU-P</pos> <!-- etc -->
			  <pos>PD</pos>
                          <pos>VRB</pos> <!-- ger -->
                    </posForValue>
                </featureValue>
                <featureValue desc="Non-genitive (modern adjectives only)">
                    <value>norm</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
              <featureValue desc="Vocative (Old Dutch only)">
                    <value>voc</value>
                    <posForValue>
                          <pos>AA</pos>
                          <pos>NOU-C</pos>
                          <pos>NOU-P</pos> <!-- etc -->
                    </posForValue>
                </featureValue>
            </values>
        </feature>

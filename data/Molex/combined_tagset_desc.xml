<tagset desc="Molex, combining lemma and wordform gigpos">
    <prefix>chnpos</prefix>
    <mainPoS>
	<pos desc="Affix" used="false">AFF</pos>
        <pos desc="Adjective/adverb">AA</pos>
        <pos desc="Adposition">ADP</pos>
        <pos desc="Adverb">ADV</pos>
        <pos desc="Polylexical">COLL</pos>
        <pos desc="Conjunction">CONJ</pos>
        <pos desc="Interjection">INT</pos>
        <pos desc="Common noun">NOU-C</pos>
        <pos desc="Proper noun">NOU-P</pos>
	<pos desc="Other noun (?)">NOU</pos>
        <pos desc="Numeral">NUM</pos>
        <pos desc="Pronoun/determiner">PD</pos>
        <pos desc="Residual">RES</pos>
        <pos desc="Verb">VRB</pos>
    </mainPoS>
    <partitions>
        <feature desc="Number subject agreement (Verbs, adjectives, pronouns, numerals)">
            <name>NA</name>
            <values>
                <featureValue desc="Plural">
                    <value>pl</value>
                    <posForValue>
                        <pos>VRB</pos>
			<pos>NUM</pos>
			<pos>AA</pos>
			<pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Singular">
                    <value>sg</value>
                    <posForValue>
                        <pos>VRB</pos>
			<pos>NUM</pos>
			<pos>AA</pos>
			<pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Unclear">
                    <value>other</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Person subject agreement (Verbs)">
            <name>PA</name>
            <values>
                <featureValue desc="First person">
                    <value>1</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Second person">
                    <value>2</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Third person">
                    <value>3</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
    </feature>

<tagset desc="Molex, combining lemma and wordform gigpos">
    <prefix>chnpos</prefix>
    <mainPoS>
        <pos desc="Adjective/adverb">AA</pos>
        <pos desc="Adposition">ADP</pos>
        <pos desc="Adverb">ADV</pos>
        <pos desc="Polylexical">COLL</pos>
        <pos desc="Conjunction">CONJ</pos>
        <pos desc="Interjection">INT</pos>
        <pos desc="Common noun">NOU-C</pos>
        <pos desc="Proper noun">NOU-P</pos>
        <pos desc="Numeral">NUM</pos>
        <pos desc="Pronoun/determiner">PD</pos>
        <pos desc="Residual">RES</pos>
        <pos desc="Verb">VRB</pos>
    </mainPoS>
    <partitions>
        <feature desc="Number subject agreement (Verbs)">
            <name>NA</name>
            <values>
                <featureValue desc="Plural">
                    <value>pl</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Singular">
                    <value>sg</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Person subject agreement (Verbs)">
            <name>PA</name>
            <values>
                <featureValue desc="First person">
                    <value>1</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Second person">
                    <value>2</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Third person">
                    <value>3</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Preferred article">
            <name>art</name>
            <values>
                <featureValue desc="Preferred article 'de'">
                    <value>de</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Preferred article 'het'">
                    <value>het</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Case">
            <name>case</name>
            <values>
                <featureValue desc="Dative or accusative">
                    <value>dat/acc</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Genitive">
                    <value>gen</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Non-genitive (adjectives)">
                    <value>norm</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Degree of comparison">
            <name>degree</name>
            <values>
                <featureValue desc="Comparative">
                    <value>comp</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Positive">
                    <value>pos</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Superlative">
                    <value>sup</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Verb finiteness">
            <name>finiteness</name>
            <values>
                <featureValue desc="Finite forms">
                    <value>fin</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Infinitive">
                    <value>inf</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Infinitive or gerund (!NORMALIZE)">
                    <value>inf/ger</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Participle">
                    <value>part</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Gender">
            <name>gender</name>
            <values>
                <featureValue desc="Feminine">
                    <value>f</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Feminine or neuter">
                    <value>f/n</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Masculine">
                    <value>m</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Masculine or feminine">
                    <value>m/f</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Masculine or feminine or neuter">
                    <value>m/f/n</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Masculine or neuter">
                    <value>m/n</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Neuter">
                    <value>n</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Inflection">
            <name>infl</name>
            <values>
                <featureValue desc="Inflection of -e (singular with definite determiner and singular indefinite neuter)">
                    <value>e</value>
                    <posForValue>
                        <pos>AA</pos>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Uninflected form (adjectives)">
                    <value>norm</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Mood (subjunctives and imperatives are not in the database)">
            <name>mood</name>
            <values>
                <featureValue desc="Indicative">
                    <value>ind</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Number">
            <name>number</name>
            <values>
                <featureValue desc="Plural">
                    <value>pl</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Singular">
                    <value>sg</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="">
            <name>pos</name>
            <values>
                <featureValue desc="">
                    <value>AA</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>ADP</value>
                    <posForValue>
                        <pos>ADP</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>ADV</value>
                    <posForValue>
                        <pos>ADV</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>COLL</value>
                    <posForValue>
                        <pos>COLL</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>CONJ</value>
                    <posForValue>
                        <pos>CONJ</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>INT</value>
                    <posForValue>
                        <pos>INT</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>NOU-C</value>
                    <posForValue>
                        <pos>NOU-C</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>NOU-P</value>
                    <posForValue>
                        <pos>NOU-P</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>NUM</value>
                    <posForValue>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>PD</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>RES</value>
                    <posForValue>
                        <pos>RES</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="">
                    <value>VRB</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Position (for Dutch verbs in the second person singular present, inflection depends on position before or after subject)">
            <name>position</name>
            <values>
                <featureValue desc="After subject (normal SV[O] order, 'je loopt')">
                    <value>as</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Before subject, in questions, object-first sentences, or sentences starting with an adverbial clause  ('loop je?', 'Dat zie je', 'Daarom loop je')">
                    <value>bs</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Separability: default is non-separable">
            <name>sep</name>
            <values>
                <featureValue desc="Separable">
                    <value>yes</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Subtype (only for numerals)">
            <name>subtype</name>
            <values>
                <featureValue desc="Fraction (derde etc)">
                    <value>fraction</value>
                    <posForValue>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Article type (definite or indefinite)">
            <name>subtype_art</name>
            <values>
                <featureValue desc="Definite">
                    <value>def</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Indefinite">
                    <value>indef</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Verb tense">
            <name>tense</name>
            <values>
                <featureValue desc="past tense; also (mis)used to characterize perfect participle">
                    <value>past</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="present tense">
                    <value>pres</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Subcategorization">
            <name>type</name>
            <values>
                <featureValue desc="Archaic">
                    <value>arch</value>
                    <posForValue>
                        <pos>ADV</pos>
                        <pos>COLL</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Auxiliary">
                    <value>aux</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Cardinal">
                    <value>card</value>
                    <posForValue>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Coordinating conjunction">
                    <value>coor</value>
                    <posForValue>
                        <pos>CONJ</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Demonstrative/relative from the 'd'-series">
                    <value>d-p</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Demonstrative">
                    <value>dem</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Indefinite">
                    <value>indef</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Interrogative">
                    <value>int</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Main verb (not auxiliary or copula)">
                    <value>mai</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Main or auxiliary verb">
                    <value>mai/aux</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Ordinal">
                    <value>ord</value>
                    <posForValue>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Other">
                    <value>other</value>
                    <posForValue>
                        <pos>RES</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Personal">
                    <value>pers</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Possessive">
                    <value>poss</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Pronominal (adverb)">
                    <value>pron</value>
                    <posForValue>
                        <pos>ADV</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Reciprocal (elkaar)">
                    <value>recip</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Reflexive">
                    <value>refl</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Relative">
                    <value>rel</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Subordinating conjunction">
                    <value>sub</value>
                    <posForValue>
                        <pos>CONJ</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="'Converted' Adjective, present participle">
                    <value>td</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="'Converted' Adjective, past participle">
                    <value>vd</value>
                    <posForValue>
                        <pos>AA</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Politeness indicator">
            <name>usage</name>
            <values>
                <featureValue desc="Polite form ('loopt u?' vs. 'loop je?')">
                    <value>u</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Written form">
            <name>wf</name>
            <values>
                <featureValue desc="Abbreviation">
                    <value>abbr</value>
                    <posForValue>
                        <pos>AA</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>COLL</pos>
                        <pos>INT</pos>
                        <pos>NOU-C</pos>
                        <pos>NOU-P</pos>
                        <pos>PD</pos>
                        <pos>RES</pos>
                    </posForValue>
                </featureValue>
                <featureValue desc="Symbol">
                    <value>symb</value>
                    <posForValue>
                        <pos>RES</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
    </partitions>
    <constraints>
        <constraint>
            <pos>AA</pos>
            <features>
                <feature>case</feature>
                <feature>degree</feature>
                <feature>infl</feature>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>ADP</pos>
            <features>
                <feature>pos</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>ADV</pos>
            <features>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>COLL</pos>
            <features>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>CONJ</pos>
            <features>
                <feature>pos</feature>
                <feature>type</feature>
            </features>
        </constraint>
        <constraint>
            <pos>INT</pos>
            <features>
                <feature>pos</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>NOU-C</pos>
            <features>
                <feature>art</feature>
                <feature>gender</feature>
                <feature>number</feature>
                <feature>pos</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>NOU-P</pos>
            <features>
                <feature>art</feature>
                <feature>gender</feature>
                <feature>number</feature>
                <feature>pos</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>NUM</pos>
            <features>
                <feature>infl</feature>
                <feature>pos</feature>
                <feature>subtype</feature>
                <feature>type</feature>
            </features>
        </constraint>
        <constraint>
            <pos>PD</pos>
            <features>
                <feature>case</feature>
                <feature>number</feature>
                <feature>person</feature>
                <feature>pos</feature>
                <feature>subtype_art</feature>
                <feature>type</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>RES</pos>
            <features>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wf</feature>
            </features>
        </constraint>
        <constraint>
            <pos>VRB</pos>
            <features>
                <feature>NA</feature>
                <feature>PA</feature>
                <feature>finiteness</feature>
                <feature>mood</feature>
                <feature>pos</feature>
                <feature>position</feature>
                <feature>sep</feature>
                <feature>tense</feature>
                <feature>type</feature>
                <feature>usage</feature>
            </features>
        </constraint>
    </constraints>
</tagset>

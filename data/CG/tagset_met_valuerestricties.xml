<tagset desc="Tagset for Corpus Gysseling in CHN style">
    <!-- we missen de display names -->
    <!-- Helaas toch maar even handmatig toevoegen? -->
    <prefix>chnpos</prefix>
    <corpus>Gysseling</corpus>
    <mainPoS>
        <pos displayName= "Adjective" desc="Adjective">ADJ</pos>
        <pos displayName= "Adposition" desc="Adposition">ADP</pos>
        <pos displayName= "Adverb" desc="Adverb">ADV</pos>
        <pos displayName= "Conjunction" desc="Conjunction" corr="CONJ">CON</pos>
        <pos displayName= "Interjection" desc="Interjection">INT</pos>
        <pos displayName= "Noun" desc="Noun">NOU</pos>
        <pos displayName= "Numeral" desc="Numeral">NUM</pos>
        <pos displayName= "Pronoun" desc="Pronoun/determiner">PD</pos>
        <pos displayName= "Residual" desc="Residual">RES</pos>
        <pos displayName= "Verb" desc="Verb">VRB</pos>
    </mainPoS>
    <partitions>
        <feature desc="Adverb type">
            <name>advType</name>
            <values>
                <featureValue displayName= "pronominal" desc="Pronominal adverb">
                    <value>pronominal</value>
                    <posForValue>
                        <pos>ADV</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Finiteness">
            <name>finiteness</name>
            <values>
                <featureValue displayName= "finite" desc="Finite verb" corr="fin">
                    <value>finite</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "infinitive/gerund" desc="Infinitive or gerund">
                    <value>inf/ger</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "participle" desc="Participle">
                    <value>part</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Inflection - categorized in an agnostic way, not necessarily implying morpheme status">
            <name>inflection</name>
            <values>
                <featureValue displayName="none" desc="No inflection">
                    <value>0</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>INT</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>RES</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "-a" desc="Inflection -a">
                    <value>a</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>CON</pos>
                        <pos>NOU</pos>
                        <pos>PD</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "-e" desc="Inflection -e">
                    <value>e</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>INT</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>RES</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "-n" desc="Inflection -n">
                    <value>n</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>INT</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>RES</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName="other" desc="Other values">
                    <value>other</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>INT</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>RES</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "-r/re" desc="Inflection -r or -re">
                    <value>r/re</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "-s/th" desc="Inflection -s or -th">
                    <value>s/th</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>RES</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue  displayName= "-t" desc="Inflection -t">
                    <value>t</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Verb mood. Only imperative has been distinguished from the indicative; tagging subjunctives turned out to be problematic">
            <name>mood</name>
            <values>
                <featureValue displayName= "imperative" desc="Imperative">
                    <value>imperative</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Number">
            <name>number</name>
            <values>
                <featureValue displayName="other" desc="Other values">
                    <value>other</value>
                    <posForValue>
                        <pos>ADJ</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName="pl" desc="Plural">
                    <value>pl</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>NOU</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName="sg" desc="Singular">
                    <value>sg</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>NOU</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Person - only tagged for pronoun/determiners">
            <name>person</name>
            <values>
                <featureValue displayName= "1" desc="First person">
                    <value>1</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue  displayName= "2" desc="Second person">
                    <value>2</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "3" desc="Third person">
                    <value>3</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature>
            <name>pos</name>
            <values>
                <featureValue>
                    <value>ADJ</value>
                    <posForValue>
                        <pos>ADJ</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>ADP</value>
                    <posForValue>
                        <pos>ADP</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>ADV</value>
                    <posForValue>
                        <pos>ADV</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>CON</value>
                    <posForValue>
                        <pos>CON</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>INT</value>
                    <posForValue>
                        <pos>INT</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>NOU</value>
                    <posForValue>
                        <pos>NOU</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>NUM</value>
                    <posForValue>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>PD</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>RES</value>
                    <posForValue>
                        <pos>RES</pos>
                    </posForValue>
                </featureValue>
                <featureValue>
                    <value>VRB</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Subtype (for pronoun/determiners)">
            <name>subtype</name>
            <values>
                <featureValue displayName= "definite" desc="Definite">
                    <value>def</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "indefinite" desc="Indefinite">
                    <value>indef</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature desc="Verb tense">
            <name>tense</name>
            <values>
                <featureValue displayName="past"  desc="Past tense">
                    <value>past</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName="present" desc="Present tense">
                    <value>present</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature  desc="Type: subcategorisation, depending on part of speech">
            <name>type</name>
            <values>
                <featureValue displayName= "-" desc="Unspecified">
                    <value>-</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "article" desc="Article">
                    <value>art</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue  displayName= "auxiliary or copula" desc="Auxiliary or copula">
                    <value>aux/cop</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue  displayName= "cardinal" desc="Cardinal">
                    <value>cardinal</value>
                    <posForValue>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "common" desc="Common noun">
                    <value>common</value>
                    <posForValue>
                        <pos>NOU</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "comparison" desc="Comparing conjunction (als, dan, gelijk-dat, alsof, ...)">
                    <value>comparison</value>
                    <posForValue>
                        <pos>CON</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "coordinating" desc="Coordinating conjunction">
                    <value>coordinating</value>
                    <posForValue>
                        <pos>CON</pos>
                    </posForValue>
                </featureValue>
                <featureValue  displayName= "demonstrative" desc="Demonstrative">
                    <value>dem</value>
                    <posForValue>
                        <pos>ADV</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "explicative" desc="Explicative">
                    <value>explicative</value>
                    <posForValue>
                        <pos>CON</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "general" desc="General" >
                    <value>general</value>
                    <posForValue>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName="indefinite" desc="Indefinite (pronoun/determiner)">
                    <value>indef</value>
                    <posForValue>
                        <pos>ADV</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "interrogative" desc="Interrogative">
                    <value>int</value>
                    <posForValue>
                        <pos>ADV</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue  displayName= "interjection" desc="Interjective (this is useless, omit?)">
                    <value>interjection</value>
                    <posForValue>
                        <pos>INT</pos>
                    </posForValue>
                </featureValue>
                <featureValue  displayName= "main" desc="Main verb (not an auxiliary of copula)">
                    <value>main</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "negative" desc="Negative">
                    <value>negative</value>
                    <posForValue>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "ordinal" desc="Ordinal">
                    <value>ordinal</value>
                    <posForValue>
                        <pos>NUM</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "other" desc="Other">
                    <value>other</value>
                    <posForValue>
                        <pos>ADP</pos>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                        <pos>RES</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "personal" desc="Personal (pronoun/determiner)">
                    <value>pers</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "possessive" desc="Possessive">
                    <value>poss</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "proper" desc="Proper noun">
                    <value>proper</value>
                    <posForValue>
                        <pos>NOU</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "quality" desc="Qualitative conjunction 'als' in the sense 'in the quality of'">
                    <value>quality</value>
                    <posForValue>
                        <pos>CON</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "refl/recp" desc="Reflective of reciprocal pronoun/determiner">
                    <value>refl/recp</value>
                    <posForValue>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "relative" desc="Relative (pronoun/determiner)">
                    <value>rel</value>
                    <posForValue>
                        <pos>ADV</pos>
                        <pos>CON</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "resumptive" desc="Resumptive">
                    <value>resumptive</value>
                    <posForValue>
                        <pos>ADV</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "subordinating" desc="Subordinating conjunction">
                    <value>subordinating</value>
                    <posForValue>
                        <pos>CON</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
        <feature displayName="Word part" desc="Description of role of token as part of a multi-token word (e.g. a separable verb)">
            <name>wordpart</name>
            <values>
                <featureValue displayName= "adpositional part" desc="Adpositional part of word">
                    <value>adp</value>
                    <posForValue>
                        <pos>ADV</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "adverbial part" desc="Adverbial part of word">
                    <value>adv</value>
                    <posForValue>
                        <pos>ADV</pos>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "part of word" desc="Unspecified part of word">
                    <value>part</value>
                    <posForValue>
                        <pos>ADJ</pos>
                        <pos>ADP</pos>
                        <pos>CON</pos>
                        <pos>NOU</pos>
                        <pos>NUM</pos>
                        <pos>PD</pos>
                    </posForValue>
                </featureValue>
                <featureValue displayName= "verbal part" desc="Verbal part of word">
                    <value>vrb</value>
                    <posForValue>
                        <pos>VRB</pos>
                    </posForValue>
                </featureValue>
            </values>
        </feature>
    </partitions>
    <constraints>
        <constraint>
            <pos>ADJ</pos>
            <features>
                <feature>inflection</feature>
                <feature>number</feature>
                <feature>pos</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
        <constraint>
            <pos>ADP</pos>
            <features>
                <feature>inflection</feature>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
        <constraint>
            <pos>ADV</pos>
            <features>
                <feature>advType</feature>
                <feature>inflection</feature>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
        <constraint>
            <pos>CON</pos>
            <features>
                <feature>inflection</feature>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
        <constraint>
            <pos>INT</pos>
            <features>
                <feature>inflection</feature>
                <feature>pos</feature>
                <feature>type</feature>
            </features>
        </constraint>
        <constraint>
            <pos>NOU</pos>
            <features>
                <feature>inflection</feature>
                <feature>number</feature>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
        <constraint>
            <pos>NUM</pos>
            <features>
                <feature>inflection</feature>
                <feature>pos</feature>
                <feature>type</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
        <constraint>
            <pos>PD</pos>
            <features>
                <feature>inflection</feature>
                <feature>number</feature>
                <feature>person</feature>
                <feature>pos</feature>
                <feature>subtype</feature>
                <feature>type</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
        <constraint>
            <pos>RES</pos>
            <features>
                <feature>inflection</feature>
                <feature>pos</feature>
                <feature>type</feature>
            </features>
        </constraint>
        <constraint>
            <pos>VRB</pos>
            <features>
                <feature>finiteness</feature>
                <feature>inflection</feature>
                <feature>mood</feature>
                <feature>pos</feature>
                <feature>tense</feature>
                <feature>type</feature>
                <feature>wordpart</feature>
            </features>
        </constraint>
    </constraints>
</tagset>
